          body .transparent-header.header2.sticky-menu{
            background:#ffffff;
          }
                              body .sticky-menu .main-menu ul li a {
            color: #000d38;
          }
                              body .sticky-menu .main-menu ul li.menu-item-has-children.current_page_ancestor > a::before,
          body .sticky-menu .main-menu ul li > .submenu li.current-menu-item>a,
          body .sticky-menu .main-menu ul li.menu-item-has-children.active > a::before,
          body .sticky-menu .main-menu ul li.current-menu-ancestor>a::before,
          body .sticky-menu .main-menu ul li.current-menu-ancestor>a,
          body .sticky-menu .main-menu ul li.current-menu-item>a,
          body .sticky-menu .main-menu ul li.active>a,
          body .sticky-menu .main-menu ul li:hover>a,
          body .sticky-menu .main-menu ul li:hover > a, 
          body .sticky-menu .main-menu ul li.menu-item-has-children:hover > a::before {
            color: #cb9b1d;
          }
